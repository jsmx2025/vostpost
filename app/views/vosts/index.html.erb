<div class="page-header">
  <h1>
    Vosts
    <a href="/vosts/new" class="btn btn-lg btn-success">New Vost</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Yes no</th>
        <th>Question</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @vosts.each do |vost| %>
      <tr>
        <td><%= vost.yes_no %></td>
        <td><%= vost.question_id %></td>
        <td><%= vost.user_id %></td>
        <td>
          <a href="/vosts/<%= vost.id %>" class="btn btn-primary">Show</a>
          <a href="/vosts/<%= vost.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_vost/<%= vost.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
